{
    "editor.codeActionsOnSave": {
        "source.fixAll.markdownlint": "always",
        "source.fixAll.cspell": "explicit",
        "source.fixAll.prettier": "explicit"
    },
    "css.validate": false,
    "scss.validate": false,
    "less.validate": false,
    "files.autoSave": "afterDelay",
    "markdownlint.configFile": ".markdownlint-cli2.jsonc"
}